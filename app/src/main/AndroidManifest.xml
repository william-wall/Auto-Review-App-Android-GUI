        <activity android:name=".MainMenu" />
        <activity android:name=".MainMenuUser" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".UserDetail" />
        <activity android:name=".ReviewDetail" />
        <activity android:name=".MainActivityFirebase" />
